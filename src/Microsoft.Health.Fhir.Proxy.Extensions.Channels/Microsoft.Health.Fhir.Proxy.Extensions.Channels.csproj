<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Copyright>© Microsoft Corporation. All rights reserved.</Copyright>
    <Version>0.0.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Libray that extends channels for custom operations using Microsoft.Health.Fhir.Proxy.</Description>
    <Authors>Microsoft</Authors>
    <PackageProjectUrl>https://azure.microsoft.com/en-us/services/healthcare-apis/</PackageProjectUrl>
    <PackageIcon>ApiForFhir.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/microsoft/fhir-proxy-sdk</RepositoryUrl>
    <PackageTags>FHIR</PackageTags>
    <PackageReleaseNotes>Dotnet 6 prelease candidate for custom operations frameworks.</PackageReleaseNotes>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	<OpenApiGenerateDocuments>false</OpenApiGenerateDocuments>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Messaging.EventGrid" Version="4.7.0" />
    <PackageReference Include="Azure.Messaging.EventHubs" Version="5.6.2" />
    <PackageReference Include="Azure.Messaging.EventHubs.Processor" Version="5.6.2" />
    <PackageReference Include="Azure.Messaging.ServiceBus" Version="7.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Health.Fhir.Proxy.Storage\Microsoft.Health.Fhir.Proxy.Storage.csproj" />
    <ProjectReference Include="..\Microsoft.Health.Fhir.Proxy\Microsoft.Health.Fhir.Proxy.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="../images/ApiForFhir.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
